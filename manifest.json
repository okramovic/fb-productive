{
  "manifest_version": 2,

  "name": "fb made productive",
  "description": "switch off bad parts of fb",
  "version": "0.0.1",

  "browser_action": {
   "default_icon": "icon_32_on.png",
   "default_popup": "popup.html"
  },
  "permissions": [
    "background",
    "tabs",
    "activeTab",
    "storage",
    "https://www.mocky.io/*"
    //"browserAction"
   ],
  "background":{
    "scripts": ["bck.js"],
    "persistent": false
  }
}